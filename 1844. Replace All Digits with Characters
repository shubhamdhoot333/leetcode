var replaceDigits = function(s) {
    var arr=[],index=0,arr1=[];
    arr=s.split("")
    console.log(arr)
for(var i=0;i<arr.length;i=i+2)
{
            if(arr[i+1])
            {
            var demo= arr[i].charCodeAt(index);
           
          var value=demo+(arr[i+1]*1);
        arr1.push(arr[i])    
          arr1.push(String.fromCharCode(value))    
            }
            else
            {
                arr1.push(arr[i])
            }
            
}
return arr1.join("")
    
};
