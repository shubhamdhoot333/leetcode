var searchRange = function(nums, target) {
   
    var arr=nums.sort(function(a, b){return a - b});
    var arr1=[],flag=0;
    var small=0,large=0;
   
    for(var i=0;i<=arr.length;i++)
    {
        
       if(arr[i]==target)
       {
         arr1.push(i)  
         flag++;
       }
       }
    if(flag==0)
    {
        return [-1,-1]
    }
    
    var len=arr1.length
    return [arr1[0],arr1[len-1]];
};
