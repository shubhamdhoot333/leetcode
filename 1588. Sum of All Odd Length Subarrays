var sumOddLengthSubarrays = function(arr) {
    var sum=0;
    for(var i=0;i<arr.length;i++)
    {
        for(var j=i;j<arr.length;j=j+2)
        {
            for(var k=i;k<=j;k++)
            {
              sum=arr[k]+sum;
              console.log(sum,arr[k])
            }
            
        }
    }
};
