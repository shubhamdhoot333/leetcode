   
var defangIPaddr = function(address) {
    str=[];
    for(var i=0;i<address.length;i++)
    {
        if(address[i]==".")
        {
            console.log("yes");
            str.push("[.]");
        }
        else
        {
            str.push(address[i]);
        }
    }
    var str1=str.join('');
    return str1;
};
