var replaceElements = function(arr) {
   
    for(var i=0;i<arr.length;i++)
    { var max=0;
        for(j=i+1;j<arr.length;j++)
        {
            if(max<arr[j])
            {
                max=arr[j];
            }
           
            
        }
         if(i==arr.length-1)
            {
                arr[i]=-1;
            }
            else
            {
        arr[i]=max;
            }
    }
    return arr;
};
