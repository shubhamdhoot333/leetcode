var intersection = function(nums1, nums2) {
  var arr=[],common=[];
  for(var i=0;i<nums1.length;i++)
  {
      for(var j=0;j<nums2.length;j++)
      {
          if(nums1[i]==nums2[j])
          {
              if(!common.includes(nums1[i]))
              {
                  common.push(nums1[i]);
              }
          }
      }
  }
  return common;
    
};
